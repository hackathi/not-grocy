{
	"include": [
		"./js/**/*.js",
		"./js/**/*.ts",
		"./js/**/*.vue",
	],
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		// this enables stricter inference for data properties on `this`
		"strict": true,
		"jsx": "preserve",
		"moduleResolution": "node",
		"allowJs": true,
		"sourceMap": true,
		"outDir": "./obj",
	},
}